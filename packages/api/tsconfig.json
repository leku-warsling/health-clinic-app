{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "types": [
      "node"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@config": ["./src/config"],
      "@db": ["./src/db"],
      "@util": ["./src/util"],
      "@constants": ["./src/constants"],
    }
  }
}